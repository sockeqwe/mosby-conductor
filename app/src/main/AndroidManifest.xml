<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hannesdorfmann.mosby3.conductor.sample">

  <uses-permission android:name="android.permission.READ_CONTACTS" />



  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:name="com.hannesdorfmann.mosby3.conductor.sample.SampleApplication"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name="com.hannesdorfmann.mosby3.conductor.sample.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
  </application>

</manifest>